{
  "name": "colloq",
  "version": "0.1.0",
  "main": "lib/index.js",
  "description": "A framework for creating simple message-oriented discord bots powered by discord.js",
  "repository": "git@github.com:fapspirit/colloq.git",
  "author": "Fedorov Anton <fapspirit@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint lib --fix",
